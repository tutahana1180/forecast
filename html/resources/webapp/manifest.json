{
	"_version": "1.5.0",
	"sap.app": {
		"id": "forecast.html",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "hanatemplates.basicSAPUI5ApplicationProject",
			"version": "0.0.0"
			},
		"dataSources": {
			"data.xsodata": {
    			"uri": "/xsodata/data.xsodata/",
    			"type": "OData",
    			"settings": {
    				"odataVersion": "2.0"
    				}
				}
			}	
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_bluecrystal"
		]
	},

	"sap.ui5": {
		"rootView": {
			"viewName": "forecast.html.view.demo",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "forecast.html.i18n.i18n"
				}
			},
			"odata": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"preload": false,
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "Default",
					"defaultCountMode": "Request",
					"useBatch": false
					},
				"dataSource": "data.xsodata"
				},
			"config": {
				"type": "sap.ui.model.json.JSONModel",
				"preload": true,
				"uri": "model/config.json"
				},
			"default": {
				"type": "sap.ui.model.json.JSONModel",
				"preload": true,
				"uri": "model/default.json"
				},
			"apl_forecast": {
				"type": "sap.ui.model.json.JSONModel",
				"preload": true,
				"uri": "model/algorithms/apl/forecast.json"
				}				
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "forecast.html.view",
				"controlAggregation": "pages",
				"controlId": "idAppControl"
				},
			"routes": [{
				"name": "home",
				"pattern": "",
				"target": "demo"
				},
				{
				"name": "forecast",
				"pattern": "forecast",
				"target": "forecast"
				}],
			"targets": {
				"demo": {
					"clearAggregation": true,
					"viewName": "demo",
					"viewLevel": 1
					},
				"forecast": {
					"clearAggregation": true,
					"viewName": "algorithms.apl.forecast"
					}	
				}
			}	
		
	}
}